{
  "orchestration": {
    "Sl-Or": {
      "virtualizer_type": "GLOBAL"
    }
  },
  "adaptation": {
    "CLEAR-DOMAINS-AFTER-SHUTDOWN": false,
    "MANAGERS": [
      "SSSA"
    ],
    "SSSA": {
      "module": "escape.adapt.managers",
      "class": "UnifyDomainManager",
      "domain_name": "SSSA",
      "diff": false,
      "poll": false,
      "adapters": {
        "REMOTE": {
          "module": "escape.adapt.adapters",
          "class": "UnifyRESTAdapter",
          "prefix": "ro/sssa",
          "url": "http://localhost:8899"
        }
      }
    }
  }
}
